{
  "name": "spa-vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:unit": "vitest --project=unit",
    "test:storybook": "vitest --project=storybook",
    "test:browser": "vitest --project=browser",
    "test:browser:coverage": "vitest --project=browser --coverage",
    "test:browser:headed": "vitest --project=browser --browser.headless=false",
    "test:unit:ci": "vitest run --project=\"unit\" --reporter=verbose --reporter=junit --reporter=json --outputFile.json=./reports/unit-report.json --outputFile.junit=./reports/unit-report.xml --coverage --coverage.reporter=lcov --coverage.reportsDirectory=./coverage",
    "test:storybook:ci": "vitest run --project=\"storybook\" --reporter=verbose --reporter=junit --reporter=json --outputFile.json=./reports/storybook-report.json --outputFile.junit=./reports/storybook-report.xml --coverage --coverage.reporter=lcov --coverage.reportsDirectory=./coverage",
    "test:coverage": "vitest run --coverage",
    "preview": "vite preview",
    "lint": "eslint . --ignore-pattern  '*.d.ts' --report-unused-disable-directives --max-warnings 0",
    "storybook": "storybook dev -p 6006",
    "test:playwright": "playwright test -c playwright.config.ts",
    "test:e2e": "playwright test -c playwright.config.ts",
    "test:e2e:ui": "playwright test -c playwright.config.ts --ui"
  },
  "dependencies": {
    "@chakra-ui/icons": "2.1.1",
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "11.13.0",
    "@emotion/styled": "11.13.0",
    "@fontsource/inter": "4.5.15",
    "@tanstack/react-query": "4.36.1",
    "@xstate/react": "6.0.0",
    "framer-motion": "9.1.7",
    "i18next": "25.3.2",
    "i18next-chained-backend": "4.6.2",
    "i18next-http-backend": "3.0.2",
    "i18next-localstorage-backend": "4.2.0",
    "ky": "0.33.3",
    "query-string": "8.2.0",
    "ramda": "0.31.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "6.0.0",
    "react-i18next": "15.6.0",
    "react-router": "7.7.0",
    "xstate": "5.24.0",
    "zustand": "4.5.4"
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-docs": "9.0.18",
    "@storybook/addon-links": "9.0.18",
    "@storybook/addon-vitest": "9.0.18",
    "@storybook/react-vite": "9.0.18",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.0.0",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/ramda": "0.30.2",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/testing-library__jest-dom": "5.14.9",
    "@vitejs/plugin-react-swc": "3.10.2",
    "@vitest/browser": "^4.0.6",
    "@vitest/browser-playwright": "4.0.6",
    "@vitest/coverage-istanbul": "3.2.4",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-import-resolver-typescript": "4.3.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "eslint-plugin-react-you-might-not-need-an-effect": "0.5.4",
    "eslint-plugin-storybook": "9.0.18",
    "eslint-plugin-vitest": "0.5.4",
    "jsdom": "21.1.2",
    "msw": "2.10.4",
    "msw-storybook-addon": "2.0.5",
    "playwright": "1.56.1",
    "prettier": "3.5.3",
    "pretty-quick": "4.1.1",
    "storybook": "9.0.18",
    "storybook-addon-remix-react-router": "5.0.0",
    "typescript": "5.8.2",
    "typescript-eslint": "8.30.1",
    "vite": "6.4.1",
    "vite-plugin-checker": "0.10.1",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.6",
    "vitest-browser-react": "^2.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "last 2 versions"
    ],
    "development": [
      "last 1 version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "packageManager": "pnpm@10.13.1"
}
